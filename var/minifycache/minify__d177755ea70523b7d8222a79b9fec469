function toggleCustomValueElements(checkbox,container,excludedElements,checked){if(container&&checkbox){var ignoredElements=[checkbox];if(typeof excludedElements!='undefined'){if(Object.prototype.toString.call(excludedElements)!='[object Array]'){excludedElements=[excludedElements];}
