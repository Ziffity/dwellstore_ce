function parseSidUrl(baseUrl,urlExt){var sidPos=baseUrl.indexOf('/?SID=');var sid='';urlExt=(urlExt!=undefined)?urlExt:'';if(sidPos>-1){sid='?'+baseUrl.substring(sidPos+2);baseUrl=baseUrl.substring(0,sidPos+1);}
